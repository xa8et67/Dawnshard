{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Banner Configuration",
    "description": "Configuration for Dawnshard's banner system",
    "type": "object",
    "properties": {
        "SummonBannerOptions": {
            "type": "object",
            "properties": {
                "Banners": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "Id": {
                                "type": "integer"
                            },
                            "Start": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "IsGala": {
                                "type": "boolean"
                            },
                            "IsPrizeShowcase": {
                                "type": "boolean"
                            },
                            "PickupCharas": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/chara"
                                }
                            },
                            "PickupDragons": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/dragon"
                                }
                            },
                            "LimitedCharas": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/chara"
                                }
                            },
                            "LimitedDragons": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/dragon"
                                }
                            },
                            "TradeCharas": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/chara"
                                }
                            },
                            "TradeDragons": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/dragon"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
